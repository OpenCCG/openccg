(|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| many|n/n|||||||0|0|Mod||||||||||papers|||| papers|n||nil||pl|||2|0|Arg0|Arg1|||||||||PASS|face||| throughout|np\np/np|||||||1|1|Mod|||||Arg1|||||papers|||| the|np/n|||||||0|0|Det||||||||||country|||| country|n||||sg|||1|0|Arg1||||||||||throughout|||| )|||||||||||||||||||||||| also|s\np\(s\np)|||||||0|0|Mod||||||||||PASS|||| PASS|s[dcl]\np/(s[pss]\np)|||dcl|||pres|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| with|pp/np|||||||1|1|Arg0|||||Arg1|||||face|||| (|||||||||||||||||||||||| a|np/n|||||||0|0|Det||||||||||slowdown|||| slowdown|n||||sg|||2|0|Arg1||||||||||with|||| (|||||||||||||||||||||||| in|np\np/np|||||||1|1|Mod|||||Arg1|||||slowdown|||| (|||||||||||||||||||||||| classified&#45;ad|n/n||||sg|||0|0|Mod||||||||||spending|||| spending|n||nil||sg|||2|0|Arg1||||||||||in|||| (|||||||||||||||||||||||| a|np/n|||||||0|0|Det||||||||||category|||| for|np\np/np|||||||1|1|Mod|||||Arg1|||||category|||| newspaper|n||nil||pl|||0|0|Arg1||||||||||for|||| booming|n/n|||||||0|0|Mod||||||||||category|||| category|n||||sg|||4|0|ApposRel||||||||||spending|||| in|np\np/np|||||||1|1|Mod|||||Arg1|||||category|||| recent|n/n|||||||0|0|Mod||||||||||year|||| year|n||nil||pl|||1|0|Arg1||||||||||in|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| face|s[pss]\np/pp|||||pass||2|2|Arg1|||||Arg0|Arg1||||PASS|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| industry|n/n||||sg|||0|0|Mod||||||||||analyst|||| analyst|n||nil||pl|||1|0|Arg0||||||||||believe|||| believe|s[dcl]\np/s[dcl]|||dcl|||past|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| and|s[dcl]$\(s[dcl]$)/(s[dcl]$)|||||||2|0|Arg1||||||||||believe|||| (|||||||||||||||||||||||| PERF|s[dcl]\np/(s[pt]\np)||||||past|2|2|First|||||Arg0|Arg1||||and|||| (|||||||||||||||||||||||| out|s\np\(s\np)|||||||0|0|Mod||||||||||bottom|||| bottom|s[pt]\np|||||past||2|1|Arg1|||||Arg0|||||PERF|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| decrease|n||nil||pl|||1|0|Arg0|Arg0|Arg0|Arg1|||||||PERF|would|bottom|increase| in|np\np/np|||||||1|1|Mod|||||Arg1|||||decrease|||| ad|n/n||||sg|||0|0|Mod||||||||||spending|||| retail|n/n|||||||0|0|Mod||||||||||spending|||| spending|n||nil||sg|||2|0|Arg1||||||||||in|||| )|||||||||||||||||||||||| in|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||would|||| fact|n||nil||sg|||0|0|Arg1||||||||||in|||| would|s[dcl]\np/(s[b]\np)|||||||3|2|Next|||||Arg0|Arg1||||and|||| (|||||||||||||||||||||||| increase|s[b]\np|||||||2|1|Arg1|||||Arg1|||||would|||| (|||||||||||||||||||||||| in|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||increase|||| (|||||||||||||||||||||||| this|np/n|||||||0|0|Det||||||||||year|||| year|n||||sg|||1|0|GenOwn||||||||||quarter|||| quarter|n||||pl|||2|0|Arg1|Of|Of||||||||in|has&#45;rel|has&#45;rel|| (|||||||||||||||||||||||| third|n/n|||||||0|0|Mod||||||||||has&#45;rel|||| and|n/n\(n/n)/(n/n)|||||||2|2|GenRel||||||||||quarter|||| fourth|n/n|||||||0|0|Mod||||||||||has&#45;rel|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| until|s/s/(s[adj]\np)|||||||1|1|Mod|||||Arg1|||||believe|||| recently|s[adj]\np|||||||1|1|Arg1||||||||||until|||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| all|np/n|||||||0|0|Det||||||||||bet|||| bet|n||||pl|||1|0|Arg0|Arg0|||||||||be|off||| be|s[dcl]\np/(s[adj]\np)|||dcl|||pres|4|2||||||Arg0|Arg1||||NULL|||| off|s[adj]\np|||||||1|1|Arg1|||||Arg0|||||be|||| analyst|n||nil||pl|||0|0|Arg0||||||||||say|||| say|s[dcl]\np/s[dcl]||||||pres|1|1|ElabRel|||||Arg0|||||be|||| (|||||||||||||||||||||||| because|s\np\(s\np)/pp|||||||1|1|Mod|||||Arg1|||||be|||| (|||||||||||||||||||||||| of|pp/np|||||||1|1|Arg1|||||Arg1|||||because|||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||ownership|||| shifting|n/n|||||||0|0|Mod||||||||||ownership|||| ownership|n||||sg|||3|0|Arg1||||||||||of|||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||ownership|||| the|np/n|||||||0|0|Det||||||||||chain|||| retail|n/n|||||||0|0|Mod||||||||||chain|||| chain|n||||pl|||2|0|Arg1||||||||||of|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| but|s[dcl]$\(s[dcl]$)\punct[,]/(s[dcl]$)|||dcl||||3|0|Arg1||||||||||NULL|say||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| Edward_J._Atorino|np|PERSON|||sg|||1|0|Arg0||||||||||say|||| for|np\np/np|||||||1|1|Mod|||||Arg1|||||analyst|||| Salomon_Brothers_Inc|np|ORGANIZATION|||sg|||0|0|Arg1||||||||||for|||| industry|n/n||||sg|||0|0|Mod||||||||||analyst|||| analyst|n||nil||sg|||2|0|ApposRel||||||||||Edward_J._Atorino|||| )|||||||||||||||||||||||| say|s[dcl]\s[dcl]/np||||||past|2|2|ElabRel|||||Arg0|Arg1||||but|||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| paper|n/n||||sg|||0|0|Mod||||||||||price|||| improved|n/n|||||||0|0|Mod||||||||||price|||| price|n||nil||pl|||2|0|Arg0|Arg0|Arg2||||||||will|help|offset|| will|s[dcl]\np/(s[b]\np)|||||||2|2|First|||||Arg0|Arg1||||but|||| (|||||||||||||||||||||||| help|s[b]\np/(s[b]\np)|||||||2|2|Arg1|||||Arg0|Arg1||||will|||| (|||||||||||||||||||||||| offset|s[b]\np/np|||||||2|2|Arg1|||||Arg1|Arg2||||help|||| in|np\np/np|||||||1|1|Mod|||||Arg1|||||weakness|||| linage|n||nil||sg|||0|0|Arg1||||||||||in|||| weakness|n||nil||sg|||1|0|Arg1||||||||||offset|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||retailer|||| retailer|n||||pl|||1|0|GenOwn||||||||||problem|||| problem|n||||pl|||1|0|Arg0|Arg0|||||||||PERF|affect||| PERF|s[dcl]\np/(s[pt]\np)||||||pres|2|2|Next|||||Arg0|Arg1||||but|||| (|||||||||||||||||||||||| affect|s[pt]\np/np|||||past||2|2|Arg1|||||Arg0|Arg1||||PERF|||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||amount|||| amount|n||||sg|||3|0|Arg1|Arg1|||||||||affect|run||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||amount|||| ad|n/n||||sg|||0|0|Mod||||||||||linage|||| linage|n||nil||sg|||1|0|Arg1||||||||||of|||| (|||||||||||||||||||||||| they|np|||||||0|0|Arg0||||||||||run|||| usually|s\np/(s\np)|||||||0|0|Mod||||||||||run|||| run|s[dcl]\np/np||||||pres|3|2|GenRe