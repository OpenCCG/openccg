(|||||||||||||||||||||||| (|||||||||||||||||||||||| Rosie|np|PERSON|||sg|||0|0|Arg0||||||||||reinvent|||| reinvent|s[dcl]\np/pp/np|||dcl|||past|4|3||||||Arg0|Arg1|Arg2|||NULL|||| (|||||||||||||||||||||||| this|np/n|||||||0|0|Det||||||||||man|||| man|n||||sg|||2|0|Arg1|Arg0|Arg0|Arg0|Arg0||||||NULL|reinvent|PERF|know|may (|||||||||||||||||||||||| or|s[dcl]$\(s[dcl]$)/(s[dcl]$)|||||||2|0|whApposRel||||||||||man|||| (|||||||||||||||||||||||| may|s[dcl]\np/(s[b]\np)|||||||2|2|First|||||Arg0|Arg1||||or|||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| not|s\np\(s\np)|||||||0|0|Mod||||||||||may|||| may|s[dcl]\np/(s[b]\np)|||||||3|2|Next|||||Arg0|Arg1||||or|||| (|||||||||||||||||||||||| PERF|s[b]\np/(s[pt]\np)|||||||2|2|Arg1|Arg1||||Arg0|Arg1||||NULL|may|may|| (|||||||||||||||||||||||| know|s[pt]\np/pp|||||past||2|2|Arg1|||||Arg0|Arg2||||PERF|||| about|pp/np|||||||1|1|Arg2|||||Arg1|||||know|||| his|np/n|||||||0|0|Det||||||||||child|||| child|n||||sg|||1|0|Arg1||||||||||about|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| as|pp/np|||||||1|1|Arg2|||||Arg1|||||reinvent|||| a|np/n|||||||0|0|Det||||||||||hero|||| war|n/n||||sg|||0|0|Mod||||||||||hero|||| hero|n||||sg|||2|0|Arg1||||||||||as|||| for|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||reinvent|||| Lily|np|PERSON|||sg|||0|0|GenOwn||||||||||benefit|||| benefit|n||||sg|||1|0|Arg1||||||||||for|||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| and|s[dcl]$\(s[dcl]$)/(s[dcl]$)|||dcl||||2|0|||||||||||NULL|||| die|s[dcl]\np||||||past|2|1|First|||||Arg1|||||and|||| Rosie|np|PERSON|||sg|||0|0|Arg1|Arg0|||||||||die|young||| young|s[adj]\np|||||||1|1|GenRel|||||Arg0|||||die|||| (|||||||||||||||||||||||| Lily|np|PERSON|||sg|||0|0|Arg0|Arg0|||||||||PERF|remember||| PERF|s[dcl]\np/(s[pt]\np)||||||pres|2|2|Next|||||Arg0|Arg1||||and|||| (|||||||||||||||||||||||| remember|s[pt]\np/pp/np|||||past||3|3|Arg1|||||Arg0|Arg1|Arg2|||PERF|||| her|np|||||||0|0|Arg1||||||||||remember|||| (|||||||||||||||||||||||| as|pp/np|||||||1|1|Arg2|||||Arg1|||||remember|||| (|||||||||||||||||||||||| a|np/n|||||||0|0|Det||||||||||figure|||| romantic|n/n|||||||0|0|Mod||||||||||figure|||| figure|n||||sg|||3|0|Arg1|Arg0|Arg0||||||||as|do|interfere|| (|||||||||||||||||||||||| n&apos;t|s\np\(s\np)|||||||0|0|Mod||||||||||do|||| do|s[dcl]\np/(s[b]\np)||||||past|3|2|whApposRel|||||Arg0|Arg1||||figure|||| (|||||||||||||||||||||||| much|s\np\(s\np)|||||||0|0|Mod||||||||||interfere|||| interfere|s[b]\np/pp|||||||3|2|Arg1|||||Arg0|Arg1||||do|||| (|||||||||||||||||||||||| with|pp/np|||||||1|1|Arg1|||||Arg1|||||interfere|||| (|||||||||||||||||||||||| her|np/n|||||||0|0|Det||||||||||child|||| child|n||||sg|||1|0|GenOwn||||||||||education|||| education|n||||sg|||2|0|Arg1||||||||||with|||| on|np\np/np|||||||1|1|Mod|||||Arg1|||||education|||| the|np/n|||||||0|0|Det||||||||||street|||| street|n||||pl|||1|0|Arg1||||||||||on|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| but|s[b]$\(s[b]$)\punct[,]/(s[b]$)|||dcl||||2|0|||||||||||NULL|||| (|||||||||||||||||||||||| Ms._Cunningham|np|PERSON|||sg|||0|0|Arg0||||||||||make|||| all|s\np\(s\np)|||||||0|0|Mod||||||||||make|||| make|s[b]\np/(s[b]\np)/np||||||pres|4|3|Next|||||Arg0|Arg1|Arg2|||but|||| it|np|||||||0|0|Arg1|Arg1|||||||||make|sound||| (|||||||||||||||||||||||| sound|s[b]\np/pp|||||||2|2|Arg2|||||Arg1|Arg2||||make|||| like|pp/np|||||||1|1|Arg2|||||Arg1|||||sound|||| a|np/n|||||||0|0|Det||||||||||adventure|||| great|n/n|||||||0|0|Mod||||||||||adventure|||| adventure|n||||sg|||2|0|Arg1||||||||||like|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| seem|s[b]\np/(s[adj]\np)|||||||3|2|First|||||Arg1|Arg1||||but|||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||game|||| game|n||||pl|||3|0|Arg1|Arg0|Arg1||||||||seem|tame|play|| (|||||||||||||||||||||||| Bronx|n/n|LOCATION|||sg|||0|0|Mod||||||||||child|||| child|n||nil||pl|||1|0|Arg0||||||||||play|||| play|s[dcl]\np/np||||||past|2|2|GenRel|||||Arg0|Arg1||||game|||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| &#45;lrb&#45;|np\np/(s[ng]\np)|||||||1|1|Mod|||||Arg1|||||game|||| (|||||||||||||||||||||||| for|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||and|||| example|n||nil||sg|||0|0|Arg1||||||||||for|||| and|s[ng]$\(s[ng]$)/(s[ng]$)|||||||3|0|Arg1||||||||||&#45;lrb&#45;|||| strip|s[ng]\np/np|||||pres||2|2|Next|||||Arg2|||||and|||| them|np|||||||0|0|Arg2||||||||||strip|||| down|s\np\(s\np)|||||||0|0|Mod||||||||||hold|||| hold|s[ng]\np/np|||||pres||3|2|First|||||Arg1|||||and|||| kid|n||nil||pl|||0|0|Arg1||||||||||hold|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| tame|s[adj]\np|||||||1|1|Arg1|||||Arg0|||||seem|||| )|||||||||||||||||||||||| by|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||seem|||| today|n||nil||sg|||0|0|GenOwn||||||||||standard|||| crack|n/n||||sg|||0|0|Mod||||||||||stand