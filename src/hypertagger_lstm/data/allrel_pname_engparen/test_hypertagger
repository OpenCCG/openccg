(|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| executive|n||nil||pl|||1|0|Arg0||||||||||say|||| at|np\np/np|||||||1|1|Mod|||||Arg1|||||executive|||| Time_Warner_Inc.|np|ORGANIZATION|||sg|||0|0|GenOwn||||||||||magazine|||| weekly|n/n|||||||0|0|Mod||||||||||magazine|||| magazine|n||||sg|||2|0|Arg1||||||||||at|||| )|||||||||||||||||||||||| say|s[dcl]\np/s[dcl]|||dcl|||past|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| Time|np|ORGANIZATION|||sg|||0|0|Arg0|Arg0|Arg0|Arg0|||||||will|de&#45;emphasize|cut|increase| will|s[dcl]\np/(s[b]\np)|||||||2|2|Arg1|||||Arg0|Arg1||||say|||| (|||||||||||||||||||||||| ;|s[b]$\(s[b]$)/(s[b]$)|||||||2|0|Arg1||||||||||will|||| (|||||||||||||||||||||||| dramatically|s\np/(s\np)|||||||0|0|Mod||||||||||de&#45;emphasize|||| de&#45;emphasize|s[b]\np/np|||||||3|2|First|||||Arg0|Arg1||||;|||| (|||||||||||||||||||||||| its|np/n|||||||0|0|Det||||||||||use|||| use|n||||sg|||3|0|Arg1||||||||||de&#45;emphasize|||| in|np\np/np|||||||1|1|Mod|||||Arg1|||||use|||| subscription|n/n||||sg|||0|0|Mod||||||||||drive|||| television|n/n||||sg|||0|0|Mod||||||||||drive|||| drive|n||nil||pl|||2|0|Arg1||||||||||in|||| (|||||||||||||||||||||||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||use|||| (|||||||||||||||||||||||| electronic|n/n|||||||0|0|Mod||||||||||giveaway|||| giveaway|n||nil||pl|||2|0|Arg1||||||||||of|||| such|np\np/(np\np)|||||||0|0|Mod||||||||||as|||| as|np\np/np|||||||2|1|Mod|||||Arg1|||||giveaway|||| telephone|n||nil||pl|||0|0|Arg1||||||||||as|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| and|s[b]$\(s[b]$)\punct[;]/(s[b]$)|||||||2|0|Next||||||||||;|||| (|||||||||||||||||||||||| cut|s[b]\np/pp/pp/np|||||||4|4|First|||||Arg0|Arg1|Arg2|Arg4||and|||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||circulation|||| circulation|n||||sg|||2|0|Arg1|Arg1|||||||||cut|guarantee||| (|||||||||||||||||||||||| it|np|||||||0|0|Arg0|||||