(|||||||||||||||||||||||| (|||||||||||||||||||||||| Mr._Field|np|PERSON|||sg|||0|0|Arg0|Arg0|Arg0||||||||PERF|be|resign|| also|s\np/(s\np)|||||||0|0|Mod||||||||||PERF|||| PERF|s[dcl]\np/(s[pt]\np)|||dcl|||past|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| be|s[pt]\np/np|||||past||3|2|Arg1|||||Arg0|Arg1||||PERF|||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||chairman|||| McKesson_Corp.|np|ORGANIZATION|||sg|||0|0|Arg1||||||||||of|||| chairman|n||nil||sg|||1|0|Arg1||||||||||be|||| (|||||||||||||||||||||||| resign|s[ng]\np/np|||||pres||3|2|EmphFinal|||||Arg0|Arg1||||be|||| that|np/n|||||||0|0|Det||||||||||post|||| post|n||||sg|||1|0|Arg1||||||||||resign|||| (|||||||||||||||||||||||| after|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||resign|||| (|||||||||||||||||||||||| a|np/n|||||||0|0|Det||||||||||dispute|||| dispute|n||||sg|||3|0|Arg1||||||||||after|||| with|np\np/np|||||||1|1|Mod|||||Arg1|||||dispute|||| the|np/n|||||||0|0|Det||||||||||board|||| board|n||||sg|||1|0|Arg1||||||||||with|||| over|np\np/np|||||||1|1|Mod|||||Arg1|||||dispute|||| corporate|n/n|||||||0|0|Mod||||||||||strategy|||| strategy|n||nil||sg|||1|0|Arg1||||||||||over|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
