(|||||||||||||||||||||||| (|||||||||||||||||||||||| Mr._Weisman|np|PERSON|||sg|||0|0|Arg0||||||||||predict|||| predict|s[dcl]\np/s[dcl]|||dcl|||pres|2|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| stock|n||nil||pl|||0|0|Arg0|Arg1|Arg1|Arg1|Arg0||||||will|appear|stabilize|decline|trap will|s[dcl]\np/(s[b]\np)|||||||2|2|Arg1|||||Arg0|Arg1||||predict|||| (|||||||||||||||||||||||| appear|s[b]\np/(s[to]\np)|||||||3|2|Arg1|||||Arg1|Arg1||||will|||| (|||||||||||||||||||||||| stabilize|s[b]\np|||||||2|1|Arg1|||||Arg1|||||appear|||| (|||||||||||||||||||||||| in|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||stabilize|||| the|np/n|||||||0|0|Det||||||||||day|||| few|n/n|||||||0|0|Mod||||||||||day|||| next|n/n|||||||0|0|Mod||||||||||day|||| day|n||||pl|||3|0|Arg1||||||||||in|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| before|s\np\(s\np)/(s[ng]\np)|||||||1|1|Mod|||||Arg1|||||appear|||| (|||||||||||||||||||||||| again|s\np\(s\np)|||||||0|0|Mod||||||||||decline|||| decline|s[ng]\np|||||pres||3|1|Arg1|||||Arg1|||||before|||| trap|s[ng]\np/np|||||pres||2|2|EmphFinal|||||Arg0|Arg1||||decline|||| more|n/n|||||||0|0|Mod||||||||||investor|||| investor|n||nil||pl|||1|0|Arg1||||||||||trap|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
