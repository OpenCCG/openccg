          <prop name="pl-X"/>
        </diamond>
      </satop>
    </lf>
  </macro>
  <macro name="@pres">
    <lf>
      <satop nomvar="E">
        <diamond mode="E">
          <prop name="pres"/>
        </diamond>
      </satop>
    </lf>
  </macro>
  <macro name="@sg">
    <fs id="2">
      <feat attr="NUM" val="sg"/>
    </fs>
  </macro>
  <macro name="@sg-X">
    <lf>
      <satop nomvar="X">
        <diamond mode="NUM">
          <prop name="sg-X"/>
        </diamond>
      </satop>
