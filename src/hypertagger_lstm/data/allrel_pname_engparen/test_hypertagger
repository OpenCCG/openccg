(|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| competition|n||nil||sg|||1|0|Arg0||||||||||spell|||| (|||||||||||||||||||||||| between|np\np/np|||||||1|1|Mod|||||Arg1|||||competition|||| (|||||||||||||||||||||||| operating|n/n||||sg|||0|0|Mod||||||||||system|||| various|n/n|||||||0|0|Mod||||||||||system|||| system|n||nil||pl|||3|0|Arg1|Arg0|||||||||between|control||| (|||||||||||||||||||||||| control|s[dcl]\np/np||||||pres|2|2|whApposRel|||||Arg0|Arg1||||system|||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||function|||| basic|n/n|||||||0|0|Mod||||||||||function|||| function|n||||pl|||3|0|Arg1||||||||||control|||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||function|||| a|np/n|||||||0|0|Det||||||||||computer|||| computer|n||||sg|||1|0|Arg1||||||||||of|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| meanwhile|s/s|||||||0|0|Mod||||||||||spell|||| spell|s[dcl]\np/np|||dcl|||pres|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| trouble|n||nil||sg|||1|0|Arg1||||||||||spell|||| for|np\np/np|||||||1|1|Mod|||||Arg1|||||trouble|||| generally|np\np|||||||0|0|Mod||||||||||firm|||| software|n/n||||sg|||0|0|Mod||||||||||firm|||| firm|n||nil||pl|||2|0|Arg1||||||||||for|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| quote&#45;rel|s/punct[&apos;&apos;]/s|||dcl||||2|0|Arg1||||||||||NULL|say||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| Russ_Crabs|np|PERSON|||sg|||1|0|Arg0||||||||||say|||| an|np/n|||||||0|0|Det||||||||||analyst|||| at|np\np/np|||||||1|1|Mod|||||Arg1|||||analyst|||| Soundview_Financial_Group|np|ORGANIZATION|||sg|||0|0|Arg1||||||||||at|||| analyst|n||||sg|||2|0|ApposRel||||||||||Russ_Crabs|||| )|||||||||||||||||||||||| say|s[dcl]\s[dcl]/np||||||past|2|2|ElabRel|||||Arg0|Arg1||||quote&#45;rel|||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| and|s[dcl]$\(s[dcl]$)/(s[dcl]$)|||||||2|0|Arg||||||||||quote&#45;rel|||| create|s[dcl]\np/np||||||pres|2|2|First|||||Arg0|Arg1||||and|||| uncertainty|n||nil||sg|||0|0|Arg1||||||||||create|||| (|||||||||||||||||||||||| it|np|||||||0|0|Arg0|Arg0|||||||||create|slow||| down|s\np\(s\np)|||||||0|0|Mod||||||||||slow|||| usually|s\np/(s\np)|||||||0|0|Mod||||||||||slow|||| slow|s[dcl]\np/np||||||pres|4|2|Next|||||Arg0|Arg1||||and|||| sale|n||nil||pl|||0|0|Arg1||||||||||slow|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
