(|||||||||||||||||||||||| (|||||||||||||||||||||||| Japan|np|LOCATION|||sg|||0|0|Arg0|Arg0|Arg0||||||||PERF|agree|cut|| however|s\np/(s\np)\punct[,]/punct[,]|||||||0|0|Mod||||||||||PERF|||| PERF|s[dcl]\np/(s[pt]\np)|||dcl|||pres|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| agree|s[pt]\np/(s[to]\np)|||||past||2|2|Arg1|||||Arg0|Arg1||||PERF|||| (|||||||||||||||||||||||| cut|s[b]\np/pp/pp/np|||||||4|4|Arg1|||||Arg0|Arg1|Arg3|Arg4||agree|||| its|np/n|||||||0|0|Det||||||||||quota|||| quota|n||||sg|||1|0|Arg1||||||||||cut|||| (|||||||||||||||||||||||| previously|pp\(s\np\(s\np))|||||||2|2|Arg3|Arg2||||Arg2|||||cut|previously||| from|s\np\(s\np)/np|||||||1|1||||||Arg1|||||NULL|||| 5.9_%|np|PERCENT|||sg|||0|0|Arg1||||||||||from|||| )|||||||||||||||||||||||| to|pp/np|||||||1|1|Arg4|||||Arg1|||||cut|||| about|n/n/(n/n)|||||||0|0|Mod||||||||||5|||| 5|n/n|PERCENT||||||1|0|Mod||||||||||%|||| %|n|PERCENT|nil||sg|||1|0|Arg1||||||||||to|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
