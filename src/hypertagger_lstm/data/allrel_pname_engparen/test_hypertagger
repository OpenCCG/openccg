(|||||||||||||||||||||||| (|||||||||||||||||||||||| as|s\np\(s\np)/(s\np\(s\np))|||||||0|0|Mod||||||||||well|||| well|s\np\(s\np)|||||||1|0|Mod||||||||||hold|||| hold|s[dcl]\np/(s[adj]\np)|||dcl|||pres|4|2||||||Arg1|Arg2||||NULL|||| this|np/n|||||||0|0|Det||||||||||reversal|||| role|n/n||||sg|||0|0|Mod||||||||||reversal|||| reversal|n||||sg|||2|0|Arg1|Arg0|||||||||hold|true||| true|s[adj]\np|||||||1|1|Arg2|||||Arg0|||||hold|||| (|||||||||||||||||||||||| for|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||hold|||| (|||||||||||||||||||||||| his|np/n|||||||0|0|Det||||||||||ally|||| three|n/n|||||||0|0|Mod||||||||||ally|||| ally|n||||pl|||4|0|Arg1|Of|Of||||||||for|has&#45;rel|has&#45;rel|| (|||||||||||||||||||||||| (|||||||||||||||||||||||| liberal|n/n|||||||0|0|Mod||||||||||has&#45;rel|||| )|||||||||||||||||||||||| and|n/n\(n/n)/(n/n)|||||||2|2|GenRel||||||||||ally|||| (|||||||||||||||||||||||| moderate|n/n|||||||0|0|Mod||||||||||has&#45;rel|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| Marshall|n/n|PERSON|||sg|||0|0|Mod||||||||||has&#45;rel|||| Harry|n/n|PERSON|||sg|||0|0|Mod||||||||||and|||| and|n\n/n|||||||3|0|Arg1|Of|||||||||,|has&#45;rel||| Blackmun|n|PERSON|||sg|||0|0|First||||||||||and|||| John_Stevens|n|PERSON|||sg|||0|0|Next||||||||||and|||| )|||||||||||||||||||||||| Thurgood|n/n|PERSON|||sg|||0|0|Mod||||||||||,|||| Justices|n/n|NAME|||sg|||0|0|Mod||||||||||,|||| ,|n\(n/n)/n||nil|||||4|2|ApposRel|||||Arg1|||||ally|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| (|||||||||||||||||||||||| these|np/n|||||||0|0|Det||||||||||player|||| four|n/n|||||||0|0|Mod||||||||||player|||| player|n||||pl|||3|0|Arg0|Arg0|||||||||be|ready||| (|||||||||||||||||||||||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||three|||| them|np|||||||0|0|Arg1||||||||||of|||| three|np|||||||2|0|ApposRel||||||||||player|||| in|np\np/np|||||||1|1|Mod|||||Arg1|||||three|||| their|np/n|||||||0|0|Det||||||||||80|||| 80|n||||pl|||1|0|Arg1||||||||||in|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| but|s/s|||||||0|0|Mod||||||||||be|||| be|s[q]/(s[adj]\np)/np|||int|||pres|3|2||||||Arg0|Arg1||||NULL|||| (|||||||||||||||||||||||| ready|s[adj]\np/(s[to]\np)|||||||2|2|Arg1|||||Arg0|Arg1||||be|||| (|||||||||||||||||||||||| assume|s[b]\np/np|||||||3|2|Arg1|||||Arg1|||||ready|||| a|np/n|||||||0|0|Det||||||||||role|||| different|n/n|||||||0|0|Mod||||||||||role|||| role|n||||sg|||2|0|Arg1||||||||||assume|||| (|||||||||||||||||||||||| after|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||assume|||| (|||||||||||||||||||||||| of|np\np/np|||||||1|1|Mod|||||Arg1|||||year|||| service|n||nil||sg|||0|0|Arg1||||||||||of|||| collectively|np\np\punct[,]/punct[,]|||||||0|0|Mod||||||||||year|||| 88|n/n|||||||0|0|Mod||||||||||year|||| year|n||nil||pl|||4|0|Arg1||||||||||after|||| on|np\np/np|||||||1|1|Mod|||||Arg1|||||year|||| the|np/n|||||||0|0|Det||||||||||court|||| high|n/n|||||||0|0|Mod||||||||||court|||| court|n||||sg|||2|0|Arg1||||||||||on|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| ;|s[dcl]$\(s[dcl]$)/(s[dcl]$)|||dcl||||2|0|||||||||||NULL|||| (|||||||||||||||||||||||| appear|s[dcl]\np/(s[adj]\np)||||||pres|2|2|Next|||||Arg1|Arg1||||;|||| Justices|n/n|NAME|||pl|||0|0|Mod||||||||||and|||| and|n\n/n||nil|||||3|0|Arg1|Arg0|||||||||appear|mad||| Blackmun|n|PERSON|||sg|||0|0|Next||||||||||and|||| Marshall|n|PERSON|||sg|||0|0|First||||||||||and|||| (|||||||||||||||||||||||| fighting|s[adj]\np/(s[adj]\np)||||sg|||0|0|Mod||||||||||mad|||| mad|s[adj]\np|||||||2|1|Arg1|||||Arg0|||||appear|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| (|||||||||||||||||||||||| appear|s[dcl]\np/(s[adj]\np)||||||pres|2|2|First|||||Arg1|Arg1||||;|||| Justices|n/n|NAME|||pl|||0|0|Mod||||||||||and|||| and|n\n/n||nil|||||3|0|Arg1|Arg0|||||||||appear|philosophical||| Stevens|n|PERSON|||sg|||0|0|Next||||||||||and|||| Brennan|n|PERSON|||sg|||0|0|First||||||||||and|||| (|||||||||||||||||||||||| philosophical|s[adj]\np/pp|||||||2|2|Arg1|||||Arg0|Arg1||||appear|||| about|pp/np|||||||1|1|Arg1|||||Arg1|||||philosophical|||| it|np|||||||0|0|Arg1||||||||||about|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
(|||||||||||||||||||||||| (|||||||||||||||||||||||| the|np/n|||||||0|0|Det||||||||||justice|||| four|n/n|||||||0|0|Mod||||||||||justice|||| justice|n||||pl|||2|0|Arg0|Arg0|Arg0||||||||be|join|criticize|| be|s[dcl]\np/np|||dcl|||pres|3|2||||||Arg0|Arg1||||NULL|||| no|np/n|||||||0|0|Det||||||||||newcomer|||| to|np\np/np|||||||1|1|Mod|||||Arg1|||||newcomer|||| dissent|n||nil||sg|||0|0|Arg1||||||||||to|||| newcomer|n||||pl|||2|0|Arg1||||||||||be|||| (|||||||||||||||||||||||| often|s\np/(s\np)|||||||0|0|Mod||||||||||join|||| join|s[ng]\np/np|||||pres||5|2|EmphFinal|||||Arg0|Arg1||||be|||| force|n||nil||pl|||0|0|Arg1||||||||||join|||| in|s\np\(s\np)/np|||||||1|1|Mod|||||Arg1|||||join|||| the|np/n|||||||0|0|Det||||||||||decade|||| past|n/n|||||||0|0|Mod||||||||||decade|||| decade|n||||sg|||2|0|Arg1||||||||||in|||| (|||||||||||||||||||||||| criticize|s[b]\np/np|||||||2|2|Purpose|||||Arg0|Arg1||||join|||| the|np/n|||||||0|0|Det||||||||||court|||| court|n||||sg|||1|0|GenOwn||||||||||drift|||| conservative|n/n|||||||0|0|Mod||||||||||drift|||| drift|n||||sg|||2|0|Arg1||||||||||criticize|||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )|||||||||||||||||||||||| )||||||||||||||||||||||||
